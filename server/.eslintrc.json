{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
            "error", 
            {
                "endOfLine": "auto"
            }],
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off"
    }
}
